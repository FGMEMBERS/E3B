<?xml version="1.0"?>

<!-- AWACS radar screen, by Dave Culp -->

<PropertyList>

 <path>radar.ac</path>

 <animation>
  <type>material</type>
  <object-name>button-up</object-name>
  <object-name>button-down</object-name>
  <object-name>button-left</object-name>
  <object-name>button-right</object-name>
   <emission>
     <red>0.4</red>
     <green>0.3</green>
     <blue>0.3</blue>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
   </emission> 
 </animation>    

 <!-- *** LoD *** -->
 <animation>
  <type>range</type>
  <min-m>0</min-m>
  <max-m>5</max-m>
 </animation>

</PropertyList>
